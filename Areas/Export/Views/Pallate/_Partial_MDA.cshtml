@model List<Dispatch_System.MDA>

<div class="row col-12 mb-3">
    <button type="button" class="btn btn-info btn-flat px-3 mr-3" onclick="fnSelect_DI()">Select</button>
    <button type="button" class="btn btn-danger btn-flat px-3 mr-3" onclick="fnClear_DI()">Clear</button>
    <button type="button" class="btn btn-danger btn-flat px-3 mr-3" onclick="fnClose_Modal()">Close</button>
</div>

<div class="row col-12 w-100">
    <table id="table_Common" class="table table-bordered table-striped w-100">
        <thead>
            <tr>
                <th>DI Number</th>
                <th>No. of Bags</th>
            </tr>
        </thead>
        <tbody>

            @if (Model != null && Model.Count > 0)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td>@item.di_no</td>
                        <td>@item.bag_nos</td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>
